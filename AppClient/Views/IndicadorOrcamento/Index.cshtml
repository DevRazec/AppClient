                                    <i class="material-icons text-white icon-2x mr-2">monetization_on</i>
                                </div>
                                <div class="media-body">
                                    <h4 id="DivPendenteCliente" class="text-right f-light text-white my-1"></h4>
                                </div>
                            </div>
                        </div>
                        <div class="card-body text-left text-dark bg-branco-claro">
                            <div class="mt-1 ld">Pendente Cliente</div>
                        </div>
                    </div>
                </div>

                <div class="col-12">
                    <div class="card mb-3">
                        <div class="card-header bg-azul-claro">
